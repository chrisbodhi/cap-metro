{
  "name": "cap-metrocket",
  "version": "1.0.0",
  "description": "WebSocket implementation of Cap Metro's realtime tracker",
  "main": "lib/server.js",
  "scripts": {
    "deploy": "git push heroku master",
    "dev": "chokidar '**/*.js' -c 'semistandard && ava'",
    "lint": "jshint .",
    "start": "node lib/server.js",
    "test": "ava",
    "validate": "npm ls"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "devDependencies": {
    "ava": "^0.23.0",
    "chokidar-cli": "^1.2.0",
    "precommit-hook": "^3.0.0",
    "semistandard": "^11.0.0"
  },
  "dependencies": {
    "express": "^4.16.2",
    "got": "^7.1.0",
    "ws": "^3.3.0"
  },
  "keywords": [],
  "author": "Chris Boette <cboette@gmail.com> (https://newschematic.org)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:chrisbodhi/cap-metrocket.git"
  },
  "engines": {
    "node": "8.9.0"
  }
}
